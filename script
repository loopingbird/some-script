local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

---Window
MoneyTab = Window:MakeTab({
	Name = "Money Tab",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

SpinTab = Window:MakeTab({
	Name = "Spins",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
---Window


---Tab
local Section = SpinTab:AddSection({
	Name = "Choose how much spins you want"
})

local Section = MoneyTab:AddSection({
	Name = "Max your spawning levels"
})
---Tab 


OrionLib:MakeNotification({
	Name = "Title!",
	Content = "Notification content... what will it say??",
	Image = "rbxassetid://4483345998",
	Time = 5
})


---Button
MoneyTab:AddButton({
	Name = "Spawn Rate",
	Callback = function()
      		local args = {
    [1] = "SpawnRateLevel",
    [2] = 30,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

MoneyTab:AddButton({
	Name = "Max Vehicles",
	Callback = function()
      		local args = {
    [1] = "MaxBlocksLevel",
    [2] = 30,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

MoneyTab:AddButton({
	Name = "Spawn Tier",
	Callback = function()
      		local args = {
    [1] = "SpawnTierLevel",
    [2] = 95,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

MoneyTab:AddButton({
	Name = "Auto Merge",
	Callback = function()
      		local args = {
    [1] = "AutoMergeLevel",
    [2] = 30,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

MoneyTab:AddButton({
	Name = "Lucky Merge",
	Callback = function()
      		local args = {
    [1] = "LuckyMergeLevel",
    [2] = 30,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

MoneyTab:AddButton({
	Name = "Cash Rate",
	Callback = function()
      		local args = {
    [1] = "CashRateLevel",
    [2] = 30,
    [3] = 0
}

game:GetService("ReplicatedStorage").Remotes.UpgradeEvent:FireServer(unpack(args))
  	end    
})

---Button

---Textbox
SpinTab:AddTextbox({
	Name = "Chosse your spins",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		local args = {
    [1] = "Spins",
    [2] = tonumber(Value)
}

game:GetService("ReplicatedStorage").Remotes.AddRewardEvent:FireServer(unpack(args))
	end	  
})


MoneyTab:AddTextbox({
	Name = "Chosse your cash",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		local args = {
    [1] = "Cash",
    [2] = tonumber(Value)
}

game:GetService("ReplicatedStorage").Remotes.AddValueEvent:FireServer(unpack(args))
	end	  
})


MoneyTab:AddTextbox({
	Name = "Pick your vehicles 1-100",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		local args = {
    [1] = tonumber(Value)
}

game:GetService("ReplicatedStorage").Remotes.SelectCarEvent:FireServer(unpack(args))

---Textbox

OrionLib:Init()
